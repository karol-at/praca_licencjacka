  "version": "0.2.0",
  "configurations": [
    {
      "experimentalNetworking": "off",
      "request": "launch",
      "name": "Debug Convert",
      "type": "node",
      "program": "${workspaceFolder}/src/converter/index.ts",
      "cwd": "${workspaceFolder}",
      "env": {},
      "runtimeExecutable": "C:\\Users\\kraol\\.deno\\bin\\deno.EXE",
      "runtimeArgs": [
        "run",
        "--unstable",
        "--inspect-wait",
        "--allow-all",
        "--env ${workspaceFolder}"
      ],
      "attachSimplePort": 9229
    },
    {
      "name": "Debug Analyst",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/src/analyst/main.py",
      "console": "integratedTerminal",
      "args": "'2025-05-27'"
    },

    {
      "experimentalNetworking": "off",
      "request": "launch",
      "name": "Debug Scraper",
      "type": "node",
      "program": "${workspaceFolder}/src/scraper/index.ts",
      "cwd": "${workspaceFolder}",
      "env": {},
      "runtimeExecutable": "C:\\Users\\kraol\\.deno\\bin\\deno.EXE",
      "runtimeArgs": [
        "run",
        "--unstable",
        "--inspect-wait",
        "--allow-all",
        "--env ${workspaceFolder}"
      ],
      "attachSimplePort": 9229
    }
  ]
}
